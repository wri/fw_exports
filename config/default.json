{
  "service": {
    "port": "3000"
  },
  "logger": {
    "name": "fw-logger",
    "level": "debug"
  },
  "mongodb": {
    "host": "MONGODB_HOST",
    "port": "MONGODB_PORT",
    "database": "exports"
  },
  "s3":{
    "region": "us-east-1",
    "folder": "exports",
    "bucket": "forest-watcher-files",
    "accessKeyId": "S3_ACCESS_KEY_ID",
    "secretAccessKey": "S3_SECRET_ACCESS_KEY"
  },
  "sparkpost": {
    "apiKey": "SPARKPOST_API_KEY"
  }
}